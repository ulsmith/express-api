{
	"name": "@ulsmith/express-api",
	"version": "1.0.0",
	"description": "An MVC Stack for express js architectures.",
	"homepage": "https://github.com/xxx/express-api#readme",
	"bugs": {
		"url": "https://github.com/xxx/express-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xxx/express-api.git"
	},
	"license": "MIT",
	"author": "Paul Smith",
	"type": "module",
	"exports": {
		"./Base/*": "./dist/Base/*.js",
		"./Error/*": "./dist/Error/*.js",
		"./Library/*": "./dist/Library/*.js",
		"./Middleware/*": "./dist/Middleware/*.js",
		"./Service/*": "./dist/Service/*.js",
		"./System/*": "./dist/System/*.js",
		"./Types/*": "./dist/Types/*.js"
	},
	"main": "dist.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rm -rf dist && tsc && node dist.js",
		"dist": "npm run build",
		"watch": "tsc --watch",
		"test": "NODE_OPTIONS='--experimental-vm-modules' jest",
		"test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
		"test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage"
	},
	"dependencies": {
		"jsonwebtoken": "^9.0.3"
	},
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.10.4",
		"@types/pg": "^8.16.0",
		"jest": "^30.2.0",
		"ts-jest": "^29.4.6",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@aws-sdk/client-dynamodb": "^3.0.0",
		"@aws-sdk/lib-dynamodb": "^3.0.0",
		"amqplib": "^0.10.9",
		"mysql2": "^3.15.3",
		"pg": "^8.16.3"
	},
	"overrides": {
		"glob": "^10.0.0",
		"test-exclude": {
			"glob": "^7.2.3"
		}
	},
	"typesVersions": {
		"*": {
			"Base/*": [
				"./dist/Base/*.d.ts"
			],
			"Error/*": [
				"./dist/Error/*.d.ts"
			],
			"Library/*": [
				"./dist/Library/*.d.ts"
			],
			"Middleware/*": [
				"./dist/Middleware/*.d.ts"
			],
			"Service/*": [
				"./dist/Service/*.d.ts"
			],
			"System/*": [
				"./dist/System/*.d.ts"
			],
			"Types/*": [
				"./dist/Types/*.d.ts"
			]
		}
	}
}
